#include <iostream>
#include <conio.h>
#include <cmath>

using namespace std;

int main()
{
	int a, b, c;
	
	cout << "Largest Number" << endl;
	cout << "First Number = "; cin >> a;
	cout << "Second Number = "; cin >> b;
	cout << "Third Number = "; cin >> c;

	if (a > b && b > c)
		cout << "The Largest Number is" << a << endl;
		else if (b > a && a > c)
			cout << "The Largest Number is " << b << endl;
			else
				cout << "The Largest Number is " << c << endl;
		
	_getch();
	return 0;
}
